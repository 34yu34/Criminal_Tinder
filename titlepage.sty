%Tout package doit avoir ces 2 lignes
\NeedsTeXFormat{LaTeX2e}
\ProvidesPackage{titlepage}
%les package a donner en dessous
%package pour changer les label dans les enumerations
% s'utilise avec \begin{enumerate}[label =blah \roman* \alph*] => \
\RequirePackage{enumitem}
%package pour choisir le path des images et plusieurs option graphique
\RequirePackage{graphicx}
%package pour faire des align* en math mode
\RequirePackage{amsmath}
%package pour faire des arbre
\RequirePackage{forest}
\forestset{qtree/.style={for tree={parent anchor=south, %ajoute des retour de ligne dans les arbres
 child anchor=north,align=center,inner sep=0pt}}}


%Creation des nouvelles commandes
\newcommand*{\dateremise}[1]{\gdef\Hdateremise{#1}}
\newcommand*{\prof}[1]{\gdef\Hprof{#1}}
\renewcommand*{\title}[1]{\gdef\Htitle{#1}}
\newcommand*{\subtitle}[1]{\gdef\Hsubtitle{#1}}
\renewcommand*{\author}[5]{\gdef\Hauthor{#1\hspace{5mm}#2 \\ #3\hspace{5mm}#4\\ #5}}

